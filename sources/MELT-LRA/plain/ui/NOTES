
# initial setup with Vite
npm init vue@latest
npm install
npm run dev

# D3
npm install d3 --save

# Vuetify 3
npm install 'vuetify@^3.1.14' --save
perform Manual steps from https://vuetifyjs.com/en/getting-started/installation/

# no v-data-table until Vuetify 3.1:
npm install vue3-easy-data-table

npm install material-design-icons-iconfont
npm install @mdi/font -D
npm install @mdi/js -D
